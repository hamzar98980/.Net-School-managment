<div class="section-full bg-white content-inner">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="sort-title clearfix text-center">
                    <h4> POPULAR COURSES </h4>
                </div>
                <!-- Event post Carousel with no margin -->
                <div class="section-content box-sort-in m-b30 button-example">
                    <div class="blog-carousel owl-loaded owl-theme owl-carousel mfp-gallery gallery owl-btn-center-lr owl-btn-2">



                        @foreach (var item in Model)
                        {

                            <div class="item">
                                <div class="ow-event-post date-style-2">
                                    <div class="ow-event-media dlab-img-effect zoom-slow"> <img src="@Url.Content(item.c_img)" alt="" width="200" height="200"> </div>
                                    <div class="ow-event-info ">
                                        <div class="ow-event-title">
                                            <h4 class="event-title"> <a href="javascript:void(0);" title="Video post">@Html.DisplayFor(modelItem => item.c_name)</a> </h4>
                                        </div>
                                        <div class="ow-event-text">
                                            <p>@Html.DisplayFor(modelItem => item.c_fees)</p>
                                            <p>@Html.DisplayFor(modelItem => item.c_desc)</p>
                                        </div>
                                        <div class="ow-event-readmore "> <a href="@Url.Action("Course","User")" title="READ MORE" rel="bookmark" class=" site-button-link"> READ MORE <i class="fa fa-angle-double-right"></i></a> </div>
                                    </div>
                                </div>
                            </div>

                        }





                    </div>
                </div>
            </div>
        </div>
    </div>
</div>











